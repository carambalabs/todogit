{
  "name": "todogit",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@types/jest": "^21.1.0",
    "@types/react": "^16.0.5",
    "@types/react-native": "^0.48.6",
    "@types/react-test-renderer": "^15.5.4",
    "babel-jest": "^21.0.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-native": "^4.0.0",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "concurrently": "^3.5.0",
    "jest-expo": "~20.0.0",
    "react-test-renderer": "16.0.0-alpha.12",
    "ts-jest": "^21.0.1",
    "typescript": "^2.5.2"
  },
  "scripts": {
    "start": "react-native start",
    "watch": "yarn run build -- -w",
    "ios": "yarn run build && concurrently -r 'yarn run watch' 'react-native run-ios'",
    "android": "yarn run build && concurrently -r 'yarn run watch' 'react-native run-android'",
    "test": "node node_modules/jest/bin/jest.js --watch",
    "tsc": "tsc",
    "clean": "rm -rf build",
    "build": "yarn run clean && yarn run tsc --",
    "lint": "tslint  -c ../tslint.json src/**/*.ts --no-cache"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "dependencies": {
    "react": "16.0.0-alpha.12",
    "react-native": "^0.47.0"
  }
}
